<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="arrayListResearch" class="me.eugasl.examples.collection.ListResearch">
        <constructor-arg name="list" ref="arrayList"/>
        <constructor-arg name="timer" ref="timer"/>
    </bean>

    <bean id="linkedListResearch" class="me.eugasl.examples.collection.ListResearch">
        <constructor-arg name="list" ref="linkedList"/>
        <constructor-arg name="timer" ref="timer"/>
    </bean>

    <bean id="arrayList" class="java.util.ArrayList"/>

    <bean id="linkedList" class="java.util.LinkedList"/>

    <bean id="timer" class="me.eugasl.examples.Timer"/>

</beans>
